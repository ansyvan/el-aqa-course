{
  "name": "tc-framework",
  "version": "1.0.0",
  "description": "Automated testing framework, automation TC 2024",
  "scripts": {
    "e2e": "node --max-old-space-size=4096 ./node_modules/.bin/wdio config/wdio.conf.js 2>&1 | tee test-logs.log",
    "e2e-test-suite": "node --max-old-space-size=4096 ./node_modules/.bin/wdio config/wdio.conf.js --suite test 2>&1 | tee test-logs.log",
    "test": "./node_modules/.bin/mocha --config=./api/test/regression.mocharc.json api/test/",
    "allure-report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "lint": "./node_modules/.bin/eslint \"**/*.js\" --fix --quiet"
  },
  "dependencies": {
    "@wdio/allure-reporter": "8.4.0",
    "@wdio/cli": "8.32.4",
    "@wdio/dot-reporter": "8.32.4",
    "@wdio/local-runner": "8.32.4",
    "@wdio/mocha-framework": "8.32.4",
    "@wdio/spec-reporter": "8.32.4",
    "axios-proxy-fix": "0.16.3",
    "bluebird": "3.7.2",
    "chai": "~4.3.7",
    "chai-as-promised": "~8.0.0",
    "chai-http": "4.4.0",
    "concurrently": "~8.2.2",
    "dotenv": "~16.4.5",
    "eslint": "9.7.0",
    "eslint-plugin-mocha": "10.5.0",
    "eslint-plugin-wdio": "8.37.0",
    "faker": "5.5.3",
    "glob": "10.4.3",
    "husky": "~9.1.1",
    "lodash": "4.17.21",
    "mkdirp": "~3.0.1",
    "mocha": "10.7.0",
    "mocha-multi": "~1.1.7",
    "mocha-param": "2.0.1",
    "moment": "2.29.3",
    "nodemailer": "6.9.3",
    "npm-run-all": "4.1.5",
    "pre-commit": "1.2.2",
    "uuid": "10.0.0",
    "wdio-image-comparison-service": "4.0.0",
    "wdio-intercept-service": "4.4.1",
    "winston": "3.13.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "~7.25.1"
  },
  "private": true
}
