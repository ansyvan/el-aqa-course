{
  "name": "tc-framework",
  "version": "1.0.0",
  "description": "Automated testing framework, automation TC 2021",
  "scripts": {
    "e2e": "node --max-old-space-size=4096 ./node_modules/.bin/wdio config/wdio.conf.js 2>&1 | tee test-logs.log",
    "e2e-test-suite": "node --max-old-space-size=4096 ./node_modules/.bin/wdio config/wdio.conf.js --suite test 2>&1 | tee test-logs.log",
    "test": "./node_modules/.bin/mocha --config=./api/test/regression.mocharc.json api/test/",
    "allure-report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "lint": "./node_modules/.bin/eslint \"**/*.js\" --fix --quiet"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@wdio/allure-reporter": "6.11.0",
    "@wdio/cli": "6.12.1",
    "@wdio/dot-reporter": "6.11.0",
    "@wdio/local-runner": "6.12.1",
    "@wdio/mocha-framework": "6.11.0",
    "@wdio/selenium-standalone-service": "6.12.1",
    "@wdio/spec-reporter": "6.11.0",
    "@wdio/sync": "6.11.0",
    "axios-proxy-fix": "0.16.3",
    "bcryptjs": "~2.4.3",
    "bluebird": "~3.7.2",
    "chai": "~4.3.0",
    "chai-as-promised": "~7.1.1",
    "chai-http": "~4.3.0",
    "chromedriver": "88.0.0",
    "concurrently": "~5.3.0",
    "dotenv": "~8.2.0",
    "eslint": "^9.37.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-wdio": "6.6.0",
    "faker": "~5.3.1",
    "geckodriver": "1.22.1",
    "glob": "~7.1.3",
    "gulp": "~4.0.0",
    "lodash": "~4.17.11",
    "mkdirp": "~1.0.4",
    "mocha": "8.2.1",
    "mocha-allure2-reporter": "0.0.3",
    "mocha-multi": "~1.1.3",
    "mocha-param": "^2.0.1",
    "moment": "2.29.1",
    "mongodb": "3.6.4",
    "nodemailer": "~6.4.17",
    "npm-run-all": "~4.1.5",
    "pre-commit": "~1.2.2",
    "sleep": "~6.3.0",
    "uuid": "~8.3.2",
    "wdio-chromedriver-service": "~6.0.4",
    "wdio-geckodriver-service": "~1.1.2",
    "wdio-image-comparison-service": "~2.0.0",
    "wdio-intercept-service": "~4.1.2",
    "winston": "~2.4.4"
  },
  "dependencies": {
    "husky": "~4.3.8"
  },
  "private": true
}
